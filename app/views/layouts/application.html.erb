<!DOCTYPE html>
<html>
  <head>
    <title>HotelChallenge</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= link_to("Home", root_path) %> | <%= link_to("About", about_path) %> | <%= link_to("Rooms", guestrooms_path) %> | <%= link_to("Contact", contact_path)%> 
    <p class="alert"><%= alert %></p>
    <p class="notice"><%= notice %></p>
    <%= yield %>
  </body>
</html>
